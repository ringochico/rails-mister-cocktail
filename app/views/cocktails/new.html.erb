<div id="new-body">
<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <h1 style="color: #3AB696">Signature Cocktail Creator</h1>
      <%= simple_form_for @cocktail do |f| %>
        <%= f.input :name %>
        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>
        <%= f.button :submit, class: 'btn btn-success' %>
      <% end %>
    </div>
  </div>
</div>
</div>
